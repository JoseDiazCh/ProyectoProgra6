
@model List<ProyeProgra6.Models.sp_RetornaUsuarios_Result>
@{
    ViewBag.Title = "UsuariosLista";
}

<h2>Usuarios Lista</h2>
<table class="table">
    <tr>
        <th>Nombre Completo</th>
        <th>Genero</th>
        <th>Correo</th>
        <th>Tipo de Usuario</th>
        <th>Provincia</th>
        <th>Acciones</th>


    </tr>

    @{ foreach (ProyeProgra6.Models.sp_RetornaUsuarios_Result posicionModelo
                 in this.Model)
        {
        <tr>



            <td>@posicionModelo.Nombre @posicionModelo.Apellido1 </td>
            <td>@posicionModelo.descripcionGenero</td>
            <td>@posicionModelo.Correo</td>
            <td>@posicionModelo.TipoUsuario</td>
            <td>@posicionModelo.nombreProvincia</td>
            <td>
                <a href="~/Usuarios/ModificaUsuario?idUsuario=@posicionModelo.idUsuario">Modificar</a>
                <br />
                <a href="~/Usuarios/EliminaUsuario?idUsuario=@posicionModelo.idUsuario">Eliminar</a>

            </td>



        </tr>
        }
    }
</table>
<a href="~/Usuarios/UsuarioNuevo"> Registar Nueva Persona</a>

