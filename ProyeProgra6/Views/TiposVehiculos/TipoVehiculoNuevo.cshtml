
@{
    ViewBag.Title = "TipoVehiculoNuevo";
}

<h2>Agrega Nuevo Tipo De Vehiculo </h2>

<form id="frmTipoVehiculo" method="post">
    <div class="form-group">
        <label>Codigo:</label>
        <input type="text" name="codigo" id="codigo" class="form-control" />
    </div>

    <div class="form-group">
        <label>Tipo De Vehiculo :</label>
        <input type="text" name="tipo" id="tipo" class="form-control" />
    </div>

    <div class="form-group">
        <input type="submit" value="Guardar" id="btnAceptar" />
    </div>
    <a href="~/TiposVehiculos/TiposVehiculosLista"> Lista De Los Tipos De Vehiculos</a>
</form>


