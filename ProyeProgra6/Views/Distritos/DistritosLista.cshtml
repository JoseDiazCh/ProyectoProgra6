@model List<ProyeProgra6.Models.sp_RetornaDistritos_Result>
@{
    ViewBag.Title = "DistritosLista";
}
@section css{

    <link href="~/Content/imegenFondo.css" rel="stylesheet" />
}
<body class="imagenFondo">
    <!--Panel-->
    <div class="panel panel-primary"
         style="text-align: left; border: solid 5px black; border-radius: 1em; filter: drop-shadow(15px 15px 20px #444); margin-left: 40px; margin-right: 25px; margin-top: 15px">
        <!--Encabezado Del Panel-->
        <div class="panel-heading">
            <div class="row" style="display: flex">
                <div style="margin-left:95px; margin-top: 10px">

                    <div style=" margin-bottom: 15px; padding-left: 40px; padding-right: 40px">
                        <h2 class="auto-style1"> Lista de los Distritos </h2>
                    </div>
                </div>
            </div>

        </div>
        <!--Fin Encabezado Del Panel-->
        <!--Cuerpo Del Panel-->
        <div class="panel-body" style="background-color: aqua; color:black">

            <!-- Columnnas Del Panel-->
            <div class="row" style="display: flex">
                <!-- Primera Columnna Del Panel-->
                <div class="column" style="flex: 50%; padding: 10px;">
                    <table class="table">
                        <tr>
                            <th> Nombre Canton</th>
                            <th>Nombre Distrito</th>
                            <th>VC Estado</th>
                            <th>distrito Inec</th>


                        </tr>

                        @{ foreach (ProyeProgra6.Models.sp_RetornaDistritos_Result posicionModelo
                                           in this.Model)
                            {
                                <tr>
                                    <td>@posicionModelo.Canton</td>
                                    <td>@posicionModelo.nombre</td>
                                    <td>@posicionModelo.vc_Estado</td>
                                    <td>@posicionModelo.id_DistritoInec</td>

                                </tr>
                            }
                        }
                    </table>
                </div>
                <!--Fin Cuerpo Panel-->
            </div>
            <!--Fin Panel-->

        </div>

    </div>
</body>


