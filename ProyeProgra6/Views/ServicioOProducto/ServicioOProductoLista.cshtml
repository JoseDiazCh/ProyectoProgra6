
@model List<ProyeProgra6.Models.sp_RetornaServicioOProducto_Result>

@{
    ViewBag.Title = "ServicioOProductoLista";
}

<h2> Lista Del Servicio o Del Producto</h2>


<table class="table">
    <tr>
        <th>Codigo</th>
        <th>Descripcion</th>
        <th>Precio</th>
        <th>Tipo</th>
    </tr>

    @{ foreach (ProyeProgra6.Models.sp_RetornaServicioOProducto_Result posicionModelo
                   in this.Model)
        {
        <tr>
            <td>@posicionModelo.Codigo</td>
            <td>@posicionModelo.Descripcion</td>
            <td>@posicionModelo.Precio</td>
            <td>@posicionModelo.Tipo</td>
            <td>
                <a href="~/ServicioOProducto/ServicioOproductoModifica?idServProduc=@posicionModelo.idServProduc">Modificar</a>
                <br />
                <a href="~/ServicioOProducto/ServicioOproductoElimina?idServProduc=@posicionModelo.idServProduc">Eliminar</a>

            </td>

        </tr>
        }
    }
</table>
<a href="~/ServicioOProducto/ServicioOProductoNuevo"> Agregar Nuevo Servicio O Producto</a>


