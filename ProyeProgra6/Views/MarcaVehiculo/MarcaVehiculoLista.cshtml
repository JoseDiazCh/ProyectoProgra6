
@model List<ProyeProgra6.Models.sp_RetornaMarcaVehiculo_Result>

@{
    ViewBag.Title = "MarcaVehiculoLista";
   
}

<h2>Lista Las Marcas De Los Vehiculos</h2>

<table class="table">
    <tr>
        <th>Codigo</th>
        <th>Tipo</th>
        <th>Acciones</th>




    </tr>

    @{ foreach (ProyeProgra6.Models.sp_RetornaMarcaVehiculo_Result posicionModelo
                       in this.Model)
        {
            <tr>
                <td>@posicionModelo.Codigo</td>
                <td>@posicionModelo.Tipo</td>

                <td>
                    <a href="~/MarcaVehiculo/ModificaMarcaVehiculo?idMarcaVehiculos=@posicionModelo.idMarcaVehiculos">Modificar</a>
                    <br />
                    <a href="~/MarcaVehiculo/MarcaVehiculoElimina?idMarcaVehiculos=@posicionModelo.idMarcaVehiculos">Eliminar</a>

                </td>

            </tr>
        }
    }
</table>
<a href="~/MarcaVehiculo/MarcaVehiculoNueva"> Registar Nueva Marca De  Vehiculo</a>
